package com;

import java.util.Stack;

public class Test3 {

	public static void main(String[] args) {
		Stack ss = new Stack();
		ss.push(100);
		ss.push(200);
		ss.push(300);
		System.out.println(ss);
		System.out.println("remove "+ss.pop());		// remove top most element 
		System.out.println(ss);	
		System.out.println("remove "+ss.peek());		// it display top most element 
		System.out.println(ss);
	}

}
